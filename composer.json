{
    "name": "novay/cms",
    "description": "CMS for Borneo Teknomedia.",
    "type": "package",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Novay\\CMS\\": "src/"
        }, 
        "files": [
            "src/Helpers/Base.php", 
            "src/Helpers/CDN.php"
        ]
    },
    "require": {
        "kirschbaum-development/eloquent-power-joins": "^3.5",
        "laravel/scout": "^10.11",
        "protonemedia/laravel-splade": "1.4.18",
        "resend/resend-laravel": "^0.14.0",
        "novay/boilerplate": "1.0.56", 
        "ryangjchandler/laravel-cloudflare-turnstile": "^1.1", 
        "cviebrock/eloquent-sluggable": "^11.0",
        "qcod/laravel-settings": "^1.2",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-permission": "^6.9",
        "venturecraft/revisionable": "^1.41", 
        "spatie/laravel-backup": "^9.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Novay\\CMS\\Providers\\CMSServiceProvider", 
                "Novay\\CMS\\Providers\\MenuServiceProvider", 
                "Novay\\CMS\\Providers\\SettingServiceProvider"
            ], 
            "alias": {
                "CMSController": "Novay\\CMS\\CMSController"
            }
        }
    }
}